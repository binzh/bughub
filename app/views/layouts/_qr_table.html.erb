<table class="qr-table">
  <% @qr.modules.each_index do |x| -%>
      <tr>
        <% @qr.modules.each_index do |y| -%>
            <% if @qr.dark?(x, y) -%>
                <td class="qr-td qr-td-black"/>
            <% else -%>
                <td class="qr-td qr-td-white"/>
            <% end -%>
        <% end -%>
      </tr>
  <% end -%>
</table>